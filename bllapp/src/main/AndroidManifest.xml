<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.blox.graphview.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="de.hechler.bll.activity.user.BenutzerProfilActivity" android:exported="true"></activity>
        <activity
            android:name="de.hechler.bll.activity.user.ImageSelectActivity"
            android:exported="true" />
        <activity
            android:name="de.hechler.bll.activity.user.NeuerBenutzerActivity"
            android:exported="true" />
        <activity android:name="de.hechler.bll.activity.user.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.hechler.bll.activity.skillTree.ContentActivity"
            android:parentActivityName="de.hechler.bll.activity.skillTree.SkillTreeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="de.hechler.bll.activity.skillTree.SkillTreeActivity" />
        </activity>
        <activity
            android:name="de.hechler.bll.activity.strategie.StrategieActivity"
            android:parentActivityName="de.hechler.bll.activity.list.AppListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="de.hechler.bll.activity.list.AppListActivity" />
        </activity>
        <activity android:name="de.hechler.bll.activity.skillTree.SkillTreeActivity" />
        <activity
            android:name="de.hechler.bll.activity.list.AppListActivity"
            android:exported="true" />
        <activity
            android:name="de.hechler.bll.activity.list.StrategieListActivity"
            android:exported="true" />
    </application>

</manifest>